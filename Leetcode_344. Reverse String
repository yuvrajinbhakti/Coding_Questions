// Question link ->  https://leetcode.com/problems/reverse-string/

//JAVA SOLUTION

class Solution {
    public void reverseString(char[] s) {
        String str = new String(s);
        StringBuilder str1 = new StringBuilder(str);
        str1.reverse();
        for(int i=0; i<s.length; i++){
            s[i] = str1.charAt(i);
        }
        
    }
}
