//Java Solution

class Solution {
    public int countPrimes(int n) {
        boolean [] prime = new boolean[n];
        // initially every element in array is false
        // false in array means every number is prime
        int count = 0;
        for(int i=2; i < n; i++){
            if(!prime[i]){
                for(int j=i*2; j<n; j+=i){
                    prime[j] = true;
                }
                count++;
            }
        }
        return count;
    }
}
